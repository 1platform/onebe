{
  "cleanOutputDir": true,
  "categorizeByGroup": false,
  "entryPointStrategy": "expand",
  "entryPoints": [
    "./src"
  ],
  "hideGenerator": true,
  "includeVersion": true,
  "name": "Spark OneBE",
  "out": "./docs",
  "readme": "./README.md",
  "externalPattern": ["node_modules"],
  "excludePrivate": true,
  "disableSources": true,
  "exclude": [
    "**/test/*.ts",
    "./src/DB/TypeORM/Decorators/Others.ts",
    "./src/**/*.d.ts",
    "./node_modules/**/*.d.ts",
    "./node_modules/@types/node/events.d.ts"
  ]
}
