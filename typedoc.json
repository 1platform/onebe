{
  "cleanOutputDir": true,
  "categorizeByGroup": true,
  "entryPointStrategy": "expand",
  "entryPoints": [
    "./src"
  ],
  "hideGenerator": true,
  "includeVersion": true,
  "name": "OneBE Framework",
  "out": "./docs",
  "readme": "./README.md",
  "externalPattern": [
    "node_modules"
  ],
  "excludePrivate": true,
  "disableSources": true,
  "exclude": [
    "**/tests/*.ts",
    "./*.test.ts",
    "./jest.config.ts",
    "./*.spec.ts",
    "./DB/TypeORM/Decorators/Others.ts",
    "./**/*.d.ts",
    "./**/yarn*.cjs",
    "./node_modules/**/*.d.ts",
    "./node_modules/@types/node/events.d.ts"
  ]
}
