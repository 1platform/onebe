    "i18next-fs-backend": "^2.1.5",
    "i18next-http-middleware": "^3.3.2",
    "inquirer": "^9.2.10",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.0.0",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "mssql": "^10.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.0",
    "nanoid": "^4.0.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.5",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
