{
  "name": "@sparkdev/onebe",
  "version": "1.0.6",
  "packageManager": "yarn@3.0.1",
  "description": "One backend framework, for all our internal needs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "scripts": {
    "docs:gen": "typedoc --tsconfig ./tsconfig.json --options ./typedoc.json",
    "docs:dev": "yarn docs:gen --watch & yarn docs:serve",
    "docs:serve": "serve docs -l 9600",
    "lint": "eslint --max-warnings 0 \"./src/**/*.ts\"",
    "lint:fix": "eslint --max-warnings 0 --fix \"./src/**/*.ts\"",
    "fix": "yarn prettier && yarn lint:fix",
    "prettier": "prettier --write ./src",
    "compile": "babel --extensions \".ts,.tsx\" --config-file \"./.babelrc.json\" src -d dist/ --copy-files",
    "compile:cleanup": "rm -rf ./dist",
    "compile:check": "tsc",
    "i18n": "sync-i18n --files 'src/locales/*.json' --primary en --languages en,ro --space 2",
    "i18n:check": "yarn i18n --check",
    "prepare": "husky install",
    "build": "yarn fix && ./bin/build.sh && git add --force docs dist && git add . && git status",
    "publish": "yarn publish:major",
    "publish:patch": "npm version patch",
    "publish:minor": "npm version minor",
    "publish:major": "npm version major",
    "postversion": "./bin/publish.sh"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spark-development/onebe.git"
  },
  "keywords": [
    "onebe",
    "backend",
    "framework",
    "typescript"
  ],
  "author": {
    "email": "hello@onebe.dev",
    "name": "Spark Development",
    "url": "https://onebe.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spark-development/onebe/issues"
  },
  "homepage": "https://github.com/spark-development/onebe#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix && yarn prettier && git add .",
      "pre-push": "yarn lint && yarn test"
    }
  },
  "dependencies": {
    "@vonage/server-sdk": "^2.10.10",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "consign": "^0.1.6",
    "core-js": "^3.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-hbs": "^2.4.0",
    "express-session": "^1.17.2",
    "flat": "^5.0.2",
    "helmet": "^4.6.0",
    "http-errors": "^1.8.0",
    "i18next": "^21.4.1",
    "i18next-fs-backend": "^1.1.1",
    "i18next-http-middleware": "^3.1.4",
    "joi": "^17.4.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.4",
    "mongoose": "^6.0.12",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "path-to-regexp": "^6.2.0",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "regenerator-runtime": "^0.13.9",
    "shortid": "^2.2.16",
    "signed": "^1.0.3",
    "speakeasy": "^2.0.0",
    "string-format": "^2.0.0",
    "string-strip-html": "^8.3.0",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.71.1",
    "typeorm": "^0.2.40",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/register": "^7.15.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/flat": "^5.0.2",
    "@types/i18next-fs-backend": "^1.1.2",
    "@types/joi": "^17.2.3",
    "@types/js-yaml": "^4.0.4",
    "@types/lodash": "^4.14.176",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.6",
    "@types/node-cron": "^3.0.0",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/passport-http": "^0.3.9",
    "@types/qrcode": "^1.4.1",
    "@types/shortid": "^0.0.29",
    "@types/speakeasy": "^2.0.6",
    "@types/string-format": "^2.0.0",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-file-loader": "^2.0.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "debug": "^4.3.2",
    "eslint": "^8.2.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^7.0.4",
    "i18next-json-sync": "^2.3.1",
    "prettier": "^2.4.1",
    "serve": "^13.0.2",
    "typedoc": "^0.22.8",
    "typedoc-plugin-extras": "^2.2.1",
    "typedoc-plugin-markdown": "^3.11.7",
    "typedoc-plugin-mdn-links": "^1.0.4",
    "typedoc-plugin-rename-defaults": "^0.4.0",
    "typescript": "^4.4.4"
  }
}
