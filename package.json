    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.6.4",
    "babel-plugin-add-module-exports": "^1.0.4",
