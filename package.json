{
  "name": "@sparkdev/onebe",
  "version": "2.7.7",
  "packageManager": "yarn@3.6.3",
  "description": "One backend framework, for all our internal needs",
  "type": "module",
  "scripts": {
    "docs:gen": "typedoc --tsconfig ./tsconfig.json --options ./typedoc.json",
    "test": "yarn test:runner --no-cache --detectOpenHandles",
    "test:runner": "TZ=UTC NODE_OPTIONS=\"--experimental-vm-modules\" jest --config ./src/jest.config.ts --runInBand",
    "test:watch": "yarn test:runner --watchAll",
    "test:multithread": "yarn test --maxConcurrency=3",
    "test:multithread:watch": "yarn test:runner --watchAll --maxConcurrency=3",
    "test:ci": "yarn test:runner --no-cache --detectOpenHandles",
    "test:ci:multithread": "yarn test:runner --clearCache && RETRY_TIMES=2 yarn test:multithread",
    "test:coverage": "yarn test --coverage",
    "code:lint": "eslint -f unix --max-warnings 0 \"./src/**/*.ts\"",
    "code:lint:fix": "eslint -f unix --max-warnings 0 --fix \"./src/**/*.ts\"",
    "code:fix": "yarn code:prettier && yarn code:lint:fix",
    "code:prettier": "prettier --write ./src",
    "app:compile": "babel --extensions \".ts,.tsx\" --config-file \"./babel.config.json\" src -d dist/ --copy-files",
    "app:compile:cleanup": "rm -rf ./dist",
    "app:compile:check": "tsc && tsc-alias",
    "app:build": "yarn code:fix && ./bin/build.sh && git add --force docs && git add . && git status",
    "app:build:link": "yarn code:fix && ./bin/build.sh && node ./bin/buildLink.js",
    "lang:i18n": "sync-i18n --files 'src/locales/*.json' --primary en --languages en,ro --space 2",
    "lang:i18n:check": "yarn lang:i18n --check",
    "prepare": "husky install",
    "publish": "yarn publish:patch",
    "publish:patch": "npm version --no-git-tag-version patch",
    "publish:minor": "npm version --no-git-tag-version minor",
    "publish:major": "npm version --no-git-tag-version major",
    "postversion": "./bin/publish.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1platform/onebe.git"
  },
  "keywords": [
    "onebe",
    "backend",
    "framework",
    "typescript"
  ],
  "author": {
    "email": "hello@onebe.dev",
    "name": "Spark Development",
    "url": "https://onebe.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1platform/onebe/issues"
  },
  "homepage": "https://onebe.dev",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.22.15",
    "@babel/core": "^7.22.15",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/node": "^7.22.15",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.22.15",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@babel/register": "^7.22.15",
    "@vonage/server-sdk": "^3.9.2",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "chalk": "^5.3.0",
    "core-js": "^3.32.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "flat": "^5.0.2",
