    "prettier": "^3.0.3",
    "serve": "^14.2.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typedoc": "^0.25.1",
    "typedoc-plugin-extras": "^3.0.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "typedoc-plugin-mdn-links": "^3.1.0",
    "typedoc-plugin-rename-defaults": "^0.6.6",
    "typescript": "^5"
  },
  "ts-standard": {
    "project": "tsconfig.json",
    "globals": [
      "expect",
      "jest",
      "test",
      "describe",
      "beforeEach"
    ]
  }
}
